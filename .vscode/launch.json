{
   // Use IntelliSense to find out which attributes exist for C# debugging
   // Use hover for the description of the existing attributes
   // For further information visit https://github.com/OmniSharp/omnisharp-vscode/blob/master/debugger-launchjson.md
   "version": "0.2.0",
   "compounds": [
    {
        "name": ".Net Core With FE Debugging in VSCode",
        "configurations": [
            ".NET Core Launch (web)",
            "Launch Chrome"
        ]
    }],
   "configurations": [
   
       {
           "type": "chrome",
           "request": "launch",
           "name": "Launch Chrome",
           "url": "http://localhost:60655/models/viewer/1",
           "webRoot": "${workspaceRoot}/ModelRelief/wwwroot",
           "sourceMaps": true
           // N.B. outFiles, outDir are not recognized by Intellisense.
           // "outDir": "${workspaceRoot}/ModelRelief/wwwroot/js"
       },
        {
            "name": ".NET Core Launch (web)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "ASPNET .Core Build",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceRoot}/ModelRelief/bin/Debug/netcoreapp2.0/ModelRelief.dll",
            "args": [],
            "cwd": "${workspaceRoot}/ModelRelief",
            "stopAtEntry": false,
            "enableStepFiltering": false,
            "console": "externalTerminal",
            "internalConsoleOptions": "openOnSessionStart",
            "launchBrowser": {
                // N.B. Set to true when using independently of the compound profiles.
                "enabled": true,
                "args": "${auto-detect-url}",
                "windows": {
                    "command": "cmd.exe",
                   "args": "/C start ${auto-detect-url}",
                // "args": "/C start http://localhost:60655/models/viewer/1"                   
                },
                "osx": {
                    "command": "open"
                },
                "linux": {
                    "command": "xdg-open"
                }
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Test",
                "InitializeDatabase":"False",
                "InitializeUserStore":"False",
            },
            "sourceFileMap": {
                "/Views": "${workspaceRoot}/Views"
            },
            "requireExactSource": true
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            // https: //github.com/OmniSharp/omnisharp-vscode/issues/897
            // "processId": "${command:pickProcess}",
            "processId": "43636"
        },
        {
            "name": "Python",
            "type": "python",
            "pythonPath": "${config:python.pythonPath}",
            "request": "launch",
            "stopOnEntry": true,
            "console": "none",
            "program": "${file}",
            "cwd": "${workspaceRoot}",
            "debugOptions": [
                "DebugStdLib",
                "RedirectOutput"
            ],
            "env": {
                "name": "value"
            },
            "args":["-s", "Test/Mesh10.json", "-w", "Test/Working/"]
        },
        {
            "name": "Node Workbench",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/NodeWorkbench/workbench.js",
            "outFiles": [
                "${workspaceFolder}/NodeWorkbench/**/*.js"
            ]
        }
    ]
}