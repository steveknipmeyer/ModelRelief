{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      // IIS Express ports should be in the range 44300 - 44399.
      // https://stackoverflow.com/questions/41075478/visual-studio-2017-enable-ssl
      "applicationUrl": "http://localhost:44300",
      "sslPort": 44301
    }
  },
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Production",
        "MRDatabaseProvider": "SQLServer",

        "MRUpdateSeedData": "False",
        "MRInitializeDatabase": "True",
        "MRSeedDatabase": "True",
        "MRExitAfterInitialization": "False"
      }
    },
    "ModelRelief Production": {
      "commandName": "Project",
      "launchBrowser": false,
      "environmentVariables": {
      "ASPNETCORE_ENVIRONMENT": "Development",
        "MRDatabaseProvider": "SQLite",

        "MRUpdateSeedData": "False",
        "MRInitializeDatabase": "True",
        "MRSeedDatabase": "True",
        "MRExitAfterInitialization": "False"
      },
      "applicationUrl": "https://localhost:5001;http://localhost:5000"
    },
    "ModelRelief Development": {
      "commandName": "Project",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "MRDatabaseProvider": "SQLite",
        "MRUpdateSeedData": "False",
        "MRInitializeDatabase": "True",
        "MRSeedDatabase": "True",
        "MRExitAfterInitialization": "False"
      },
      "applicationUrl": "https://localhost:5001;http://localhost:5000"
    }
  }
}