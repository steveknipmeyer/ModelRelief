{
  // https://github.com/serilog/serilog/wiki/Configuration-Basics
  // "C:\Users\Steve Knipmeyer\AppData\Local\Temp\Logs\ModelRelief.log"
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Verbose",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": { "path": "%TEMP%\\Logs\\ModelRelief.log" }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "ModelRelief"
    }
  },

  // N.B. These settings are defined in the environment-specific appsetting.<Environment>.json (e.g. appsettings.Test.json).
  // ConnectionStrings
  // ResourcePaths.StoreUsers

  "ResourcePaths": {
    "ModelFolders": {
      "DepthBuffers": "depthbuffers",
      "Meshes": "meshes",
      "Models": "models"
    },
    "TestDataUsers": "/Test/Data/Users/"
  },

  "Greeting": "Hello, ModelRelief!"
}